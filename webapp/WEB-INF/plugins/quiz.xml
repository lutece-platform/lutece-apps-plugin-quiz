<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>

    <!-- Plugin Informations -->
    <name>quiz</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>2.0.0</version>
    <description>quiz.plugin.description</description>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <provider>quiz.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/quiz/quiz.png</icon-url>
    <copyright>Copyright 2001-2011 Mairie de Paris</copyright>
    <db-pool-required>1</db-pool-required>
	<core-version-dependency>
        <min-core-version>4.0.0</min-core-version>
        <max-core-version/>
    </core-version-dependency>
    <!-- Specific plugin CSS stylesheet -->
    <css-stylesheets>
        <css-stylesheets-scope>xpage</css-stylesheets-scope>
        <css-stylesheet>quiz/quiz.css</css-stylesheet>
    </css-stylesheets>

    <!-- Administration interface parameters -->
    <admin-features>
        <admin-feature>
            <feature-id>QUIZ_MANAGEMENT</feature-id>
            <feature-title>quiz.adminFeature.quiz_management.name</feature-title>
            <feature-description>quiz.adminFeature.quiz_management.description</feature-description>
            <feature-level>3</feature-level>
            <feature-url>jsp/admin/plugins/quiz/ManageQuiz.jsp</feature-url>
        </admin-feature>
    </admin-features>

    <!-- Applications -->
    <applications>
        <application>
            <application-id>quiz</application-id>
            <application-class>fr.paris.lutece.plugins.quiz.web.QuizApp</application-class>
        </application>
    </applications>

</plug-in>